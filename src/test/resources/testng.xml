<?xml version="1.0" encoding="UTF-8"?>
<suite name="Tests" thread-count="2">
    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>
    <test name="Backend Tests" parallel="false">
        <classes>
            <class name="Tests.APITests" />
        </classes>
    </test>
    <test name = "UI Tests" parallel="false">
        <classes>
            <class name = "Tests.UITests"/>
        </classes>
    </test>
    <test name = "parallel tests" parallel="true">
        <classes>
            <class name = "Tests.ParallelTestExecution"/>
        </classes>
    </test>
</suite>

